{
  "name": "vuejs-datepicker",
  "version": "0.9.29",
  "description": "A simple Vue.js datepicker component. Supports disabling of dates, inline mode, translations",
  "keywords": [
    "vue",
    "datepicker",
    "date-picker",
    "calendar"
  ],
  "main": "dist/vuejs-datepicker.js",
  "module": "dist/vuejs-datepicker.esm.js",
  "author": "Charlie Kassel <ck@charliekassel.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/charliekassel/vuejs-datepicker"
  },
  "bugs": {
    "url": "https://github.com/charliekassel/vuejs-datepicker/issues"
  },
  "private": false,
  "license": "MIT",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "rm -rf dist && node build/build.js && babel-node build/build-locale.js",
    "build-locale": "babel-node scripts/build-locale.js",
    "build-esm": "rollup -c scripts/build-esm.js",
    "build-umd": "rollup -c scripts/build-umd.js && rollup -c scripts/build-umd.min.js",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "prepublish": "npm run build"
  },
